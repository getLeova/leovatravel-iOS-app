<?xml version='1.0' encoding='utf-8'?>
<widget id="com.getleova.leova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LeovaTravel</name>
    <description>
        Leova Demo Project
    </description>
    <author email="support@getLeova.com" href="https://www.getLeova.com/">
        17E Tech
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <!-- iOS 8.0+ -->
        <!-- iPhone 6 Plus  -->
        <icon src="./www/img/icon.png" width="180" height="180" />
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="./www/img/icon.png" width="60" height="60" />
        <icon src="./www/img/icon.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="./www/img/icon.png" width="76" height="76" />
        <icon src="./www/img/icon.png" width="152" height="152" />
        <!-- iOS 6.1 -->
        <!-- Spotlight Icon -->
        <icon src="./www/img/icon.png" width="40" height="40" />
        <icon src="./www/img/icon.png" width="80" height="80" />
        <!-- iPhone / iPod Touch -->
        <icon src="./www/img/icon.png" width="57" height="57" />
        <icon src="./www/img/icon.png" width="114" height="114" />
        <!-- iPad -->
        <icon src="./www/img/icon.png" width="72" height="72" />
        <icon src="./www/img/icon.png" width="144" height="144" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="./www/img/icon.png" width="29" height="29" />
        <icon src="./www/img/icon.png" width="58" height="58" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon src="./www/img/icon.png" width="50" height="50" />
        <icon src="./www/img/icon.png" width="100" height="100" />
        
        <splash src="./www/img/logo_splash.png" width="320" height="480"/>
        <splash src="./www/img/logo_splash.png" width="640" height="960"/>
        <splash src="./www/img/logo_splash.png" width="768" height="1024"/>
        <splash src="./www/img/logo_splash.png" width="1536" height="2048"/>
        <splash src="./www/img/logo_splash.png" width="1024" height="768"/>
        <splash src="./www/img/logo_splash.png" width="2048" height="1536"/>
        <splash src="./www/img/logo_splash.png" width="640" height="1136"/>
        <splash src="./www/img/logo_splash.png" width="750" height="1334"/>
        <splash src="./www/img/logo_splash.png" width="1242" height="2208"/>
        <splash src="./www/img/logo_splash.png" width="2208" height="1242"/>
    </platform>
    <preference name="apiKey" value="developerdemokeydeveloperdemokey" />
</widget>
